{
  "name": "node-jira-cloud-action",
  "description": "GitHub Actions to communicate with Jira Cloud",
  "version": "0.0.1",
  "author": "aureolebigben",
  "type": "module",
  "private": true,
  "homepage": "https://github.com/aureolebigben/node-jira-cloud-action#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aureolebigben/node-jira-cloud-action.git"
  },
  "bugs": {
    "url": "https://github.com/aureolebigben/node-jira-cloud-action/issues"
  },
  "keywords": [
    "actions"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "bundle": "npm run format:write && npm run package",
    "ci-test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "coverage": "npx make-coverage-badge --output-path ./badges/coverage.svg",
    "format:write": "npx prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "npx eslint .",
    "local-action": "npx @github/local-action . src/main.js .env",
    "package": "npx rollup --config rollup.config.js",
    "package:watch": "npm run package -- --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 npx jest",
    "all": "npm run format:write && npm run lint && npm run test && npm run coverage && npm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^2.0.1",
    "@actions/http-client": "^2.2.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@github/local-action": "^5.1.0",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.5.0",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.7.4",
    "prettier-eslint": "^16.4.2",
    "rollup": "^4.53.5"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  }
}
